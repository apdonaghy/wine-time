,SAAA,EAAA,OADA;AAEA,QAAA,QAAA,EAAA,CAAA,cAAA,EAAA,YAAA,EAAA,iBAAA;AAFA,OAxBA;AA4BA,MAAA,SAAA,EAAA;AACA,QAAA,SAAA,EAAA,WADA;AAEA,QAAA,QAAA,EAAA,CAAA,WAAA,EAAA,MAAA,EAAA,UAAA,EAAA,gBAAA;AAFA;AA5BA,KAAA;AAiCA,GAxCA;AAyCA,EAAA,QAAA,EAAA;AACA,IAAA,aAAA,EAAA,yBAAA;AAAA;;AACA,aAAA,KAAA,OAAA,CACA,MADA,CACA,UAAA,MAAA,EAAA;AACA,eAAA,MAAA,CAAA,KAAA,GAAA,KAAA,CAAA,QAAA;AACA,OAHA,CAAA;AAMA;AARA,GAzCA;AAmDA,EAAA,OAAA,EAAA;AACA,IAAA,SAAA,EAAA,mBAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,MAAA;AACA,KAHA;AAIA,IAAA,aAAA,EAAA,yBAAA;AAAA;;AACA,WAAA,WAAA,GAAA,IAAA;AACA,MAAA,UAAA,CAAA;AAAA,eAAA,MAAA,CAAA,WAAA,GAAA,KAAA;AAAA,OAAA,EAAA,GAAA,CAAA;AACA,KAPA;AASA,IAAA,YATA,wBASA,KATA,EASA;AACA,WAAA,IAAA,GAAA,KAAA,CAAA,QAAA;AACA,WAAA,WAAA,GAAA,KAAA,CAAA,YAAA;;AACA,UAAA,KAAA,WAAA,KAAA,KAAA,EAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA;AACA,aAAA,KAAA,CAAA,SAAA,CAAA,KAAA;AACA,OAHA,MAGA,IAAA,KAAA,WAAA,KAAA,OAAA,EAAA;AACA,aAAA,KAAA,CAAA,GAAA,CAAA,KAAA;AACA,aAAA,KAAA,CAAA,SAAA,CAAA,KAAA;AACA,OAHA,MAGA,IAAA,KAAA,WAAA,KAAA,WAAA,EAAA;AACA,aAAA,KAAA,CAAA,KAAA,CAAA,KAAA;AACA,aAAA,KAAA,CAAA,GAAA,CAAA,KAAA;AACA;;AAEA,WAAA,KAAA,CAAA,SAAA,EAAA;AACA,QAAA,QAAA,EAAA,KAAA,QADA;AAEA,QAAA,IAAA,EAAA,KAAA,IAFA;AAGA,QAAA,QAAA,EAAA,KAAA;AAHA,OAAA;AAMA,UAAA,UAAA;;AACA,WAAA,IAAA,SAAA,IAAA,KAAA,OAAA,EAAA;AACA,YAAA,KAAA,OAAA,CAAA,SAAA,MAAA,IAAA,EAAA;AACA,UAAA,UAAA,GAAA,SAAA;AACA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,UAAA,EA1BA,CA4BA;AACA;AACA;AACA;AACA;AACA;AAEA;AACA;AACA;AACA;AACA;AACA;AAEA,KAnDA;AAoDA,IAAA,SApDA,qBAoDA,UApDA,EAoDA;AACA,WAAA,IAAA,SAAA,IAAA,KAAA,OAAA,EAAA;AACA,YAAA,SAAA,KAAA,UAAA,EAAA;AACA,eAAA,OAAA,CAAA,SAAA,IAAA,IAAA;AACA,SAFA,MAEA;AACA,eAAA,OAAA,CAAA,SAAA,IAAA,KAAA;AACA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,OAAA;AACA,KA7DA;AA8DA,IAAA,IA9DA,kBA8DA;AAEA,WAAA,SAAA,CAAA,SAAA;AAEA,aAAA,KAAA,OAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA,OAFA,CAAA;AAGA,KArEA;AAsEA,IAAA,GAtEA,iBAsEA;AACA,WAAA,SAAA,CAAA,SAAA;AACA,aAAA,KAAA,OAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA,OAFA,CAAA;AAGA,KA3EA;AA4EA,IAAA,cA5EA,4BA4EA;AACA,WAAA,SAAA,CAAA,SAAA;AACA,aAAA,KAAA,OAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,aAAA,GAAA,CAAA,CAAA,aAAA;AACA,OAFA,CAAA;AAGA,KAjFA;AAkFA,IAAA,WAlFA,yBAkFA;AACA,WAAA,SAAA,CAAA,SAAA;AACA,aAAA,KAAA,OAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,WAAA,GAAA,CAAA,CAAA,WAAA;AACA,OAFA,CAAA;AAGA,KAvFA;AAyFA;AACA,IAAA,WAAA,EAAA,qBAAA,OAAA,EAAA;AACA,WAAA,IAAA,IAAA,IAAA,OAAA,EAAA;AACA,QAAA,OAAA,CAAA,IAAA,CAAA,CAAA,KAAA,GAAA,UAAA,CAAA,OAAA,CAAA,IAAA,CAAA,CAAA,KAAA,CAAA,SAAA,CAAA,CAAA,CAAA,CAAA;AACA;;AACA,aAAA,OAAA;AACA;AA/FA;AAnDA,CAAA","sourcesContent":["<template>\n  <div id=\"Home\">\n    <!-- when form is submitted prevent deafault reload and trigger the findWine mehtod -->\n\n    <transition name=\"fade\" v-if=\"addedNotice\">\n      <div class=\"higher\">\n        <span class=\"higherText\">Added to collection</span>\n      </div>\n    </transition>\n\n    <div class=\"gray\">\n      <div class=\"lower flexMain\">\n        <section class=\"filter\">\n          <div class=\"flex filterHeaders\">\n          <h3 class=\"filterMainTitle\">Filters</h3>\n          <h5> Results <span v-if=\"results && results.length && !spin > 0\">{{filteredWines.length}}</span> </h5>\n          </div>\n          <Accordion\n            class=\"accMargin\"\n            ref=\"red\"\n            v-on:checkedData=\"onChildClick\"\n            v-bind:wineNames=\"reds\"\n          ></Accordion>\n          <Accordion\n            class=\"accMargin\"\n            ref=\"white\"\n            v-on:checkedData=\"onChildClick\"\n            v-bind:wineNames=\"white\"\n          ></Accordion>\n          <Accordion\n            class=\"accMargin\"\n            ref=\"sparkling\"\n            v-on:checkedData=\"onChildClick\"\n            v-bind:wineNames=\"sparkling\"\n          ></Accordion>\n\n          <div class=\"control\">\n            <h4 class=\"priceTitle\"><span>Max Price</span> ${{maxPrice}}</h4>\n            <input type=\"range\" min=\"0\" max=\"125\" v-model=\"maxPrice\" />\n          </div>\n            <div class=\"btnContainer\">\n            <button\n              :class=\"{ filterBtnPressed: filters.filter1 }\"\n              class=\"filter-btn\"\n              @click=\"high\"\n              @keyup.tab=\"high\"\n            >$ high to low\n            </button>\n            <button\n              :class=\"{ filterBtnPressed: filters.filter2 }\"\n              class=\"filter-btn\"\n              @click=\"low\"\n              @keyup.tab=\"low\"\n            >\n              $ low to high\n            </button>\n            <button\n              :class=\"{ filterBtnPressed: filters.filter3 }\"\n              class=\"filter-btn\"\n              @click=\"highestRatings\"\n              @keyup.tab=\"highestRatings\"\n            >\n              Highest Rated\n            </button>\n            <button\n              :class=\"{ filterBtnPressed: filters.filter4 }\"\n              class=\"filter-btn\"\n              @click=\"mostRatings\"\n              @keyup.tab=\"mostRatings\"\n            >\n              Rating Count\n            </button>\n          </div>\n        </section>\n\n        <!-- container for formatted api response -->\n        <main class=\"wines-container\">\n          <font-awesome-icon\n            v-show=\"spin\"\n            class=\"fa-spin spinner\"\n            icon=\"circle-notch\"\n          />\n          <!-- v-if conditional to display ul contents only if api response is valid and is > than 0 items -->\n          <ul v-if=\"results && results.length && !spin > 0\">\n          \n\n            <!-- v-for loops through the results array and injects the oject of each index into the DOM-->\n            <li\n              v-for=\"item of filteredWines\"\n              class=\"wine-container container\"\n              :key=\"item.id\"\n            >\n            \n             \n\n              <div class=\"flex-container\">\n                <div class=\"twentyFive\">\n                  <img class=\"img\" :src=\"item.imageUrl\" :alt=\"item.title\" />\n                </div>\n                <div class=\"seventyFive\">\n                  <h2>\n                    {{ item.title }}\n                  </h2>\n                  <span class=\"price flex\">\n                    ${{ item.price }} |\n                    <a class=\"purchase\" target=\"_blank\" :href=\"item.link\"\n                      >PURCHASE</a\n                    >\n                    |\n                    <div @click=\"showAddNotice\" class=\"inline\">\n                      <span\n                        @click=\"$emit('saveWine', item)\"\n                        class=\"collection_btn\"\n                        >Add to collection +</span\n                      >\n                    </div>\n                  </span>\n                  <div class=\"flex\">\n                  <span class=\"rating\">\n                    Rating\n                \n                  </span>\n                      <span class=\"inside-rating\"\n                      >{{ Math.floor(item.averageRating * 100) }}%</span\n                    >\n                  </div>\n                  \n                  <span class=\"ratings\">\n                    Ratings\n                    <strong>x {{ Math.floor(item.ratingCount) }}</strong>\n                  </span>\n\n                  <p>{{ item.description }}</p>\n                </div>\n              </div>\n            </li>\n          </ul>\n\n          <!-- If the API doesn't return result from a properly filled out form -->\n          <div v-else-if=\"results && results.length == 0\" class=\"no-results\">\n            <h2>No Wines Found</h2>\n            <p>Please adjust your search</p>\n          </div>\n\n          <!-- If the API can't return data because the form was filled out improperly -->\n          <ul v-if=\"errors && errors.length > 0\" class=\"errors\">\n            <li v-for=\"error of errors\" :key=\"error\">{{ error.message }}</li>\n          </ul>\n        </main>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Accordion from \"../components/Accordion.vue\";\n\nexport default {\n  name: \"Home\", //router\n  props: [\"results\", \"spin\", \"favorites\", \"errors\"],\n  components: {\n    Accordion,\n  },\n  data() {\n    return {\n\n      addedNotice: false,\n      wine: \"\",\n      maxPrice: 125,\n      currentList: null,\n      filters:{\n      filter1: false,\n      filter2: false,\n      filter3: false,\n      filter4: false\n      },\n      spinning: true,\n      reds: {\n        wineTitle: \"Red\",\n        wineList: [\n          \"Merlot\",\n          \"Cabernet Sauvignon\",\n          \"Syrah\",\n          \"Pinot Noir\",\n          \"Malbec\",\n          \"Zinfandel\",\n        ],\n      },\n      white: {\n        wineTitle: \"White\",\n        wineList: [\"Pinot Grigio\", \"Chardonnay\", \"Sauvignon blanc\"],\n      },\n      sparkling: {\n        wineTitle: \"Sparkling\",\n        wineList: [\"Champagne\", \"Cava\", \"Prosecco\", \"Sparkling Wine\"],\n      },\n    };\n  },\n    computed:{\n      filteredWines: function() {\n      return this.results\n        .filter(result => {\n          return result.price < this.maxPrice;\n        })\n        \n\n    }\n  },\n  methods: {\n    resetForm: function (event) {\n      console.log(event.target);\n    },\n    showAddNotice: function () {\n      this.addedNotice = true;\n      setTimeout(() => (this.addedNotice = false), 500);\n    },\n\n    onChildClick(value) {\n      this.wine = value.inputVal;\n      this.currentList = value.currentTitle;\n      if (this.currentList === \"Red\") {\n        this.$refs.white.clear();\n        this.$refs.sparkling.clear();\n      } else if (this.currentList === \"White\") {\n        this.$refs.red.clear();\n        this.$refs.sparkling.clear();\n      } else if (this.currentList === \"Sparkling\") {\n        this.$refs.white.clear();\n        this.$refs.red.clear();\n      }\n\n      this.$emit(\"apiCall\", {\n        maxPrice: this.maxPrice,\n        wine: this.wine,\n        spinning: this.spinning,\n      });\n\n      let thisFilter;\n        for(let oneFilter in this.filters){\n         if(this.filters[oneFilter] === true){\n            thisFilter = oneFilter\n         }\n        }\n        console.log(thisFilter)\n  \n        // if(thisFilter === this.filters.filter1){\n        //   this.high()\n        // }\n        // if(thisFilter === this.filters.filter2){\n        //   this.low()\n        // }\n\n        // if(thisFilter === this.filters.filter3){\n        //   this.highestRatings()\n        // }\n        //  if(thisFilter === this.filters.filter4){\n        //   this.mostRatings()\n        // }\n\n    },\n    filterBtn(filterName){\n        for(let oneFilter in this.filters){\n         if(oneFilter === filterName){\n               this.filters[oneFilter] = true;\n         }else{\n              this.filters[oneFilter] = false;\n         }\n       }\n       console.log(this.filters)\n    },\n    high() {\n   \n      this.filterBtn('filter1')\n\n      return this.results.sort(function (a, b) {\n        return b.price - a.price;\n      });\n    },\n    low() {\n      this.filterBtn('filter2')\n      return this.results.sort(function (a, b) {\n        return a.price - b.price;\n      });\n    },\n    highestRatings() {\n      this.filterBtn('filter3')\n      return this.results.sort(function (a, b) {\n      return b.averageRating - a.averageRating;\n      });\n    },\n    mostRatings() {\n      this.filterBtn('filter4')\n      return this.results.sort(function (a, b) {\n        return b.ratingCount - a.ratingCount;\n      });\n    },\n\n    //This function takes each index in the results array and changes the results.price from a string to a number so that filter methods can compare them properly.\n    processData: function (dataArr) {\n      for (let data in dataArr) {\n        dataArr[data].price = parseFloat(dataArr[data].price.substring(1));\n      }\n      return dataArr;\n    },\n  },\n};\n</script>\n\n\n<style scoped>\n/* styling for this specific view */\n\nbutton{\n\nbackground-color:transparent;\n}\n.filterHeaders{\n  justify-content: space-between;\n    border-bottom: solid 1px rgba(0,0,0,0.1);\n  padding-bottom:10px;\n  margin-bottom:20px;\n}\n\nh5{\n  font-size:.9em;\n  font-family: var(--sansSerif);\n  font-weight:600;\n  color:gray;\n}\n\n.filterMainTitle {\n  font-family: var(--sansSerif);\n  font-size: 1.2em;\n  font-weight: 200;\n}\n\n\n\ninput[type=range] {\n  -webkit-appearance: none;\n  margin: 10px 0;\n  width: 100%;\n}\ninput[type=range]:focus {\n  outline: none;\n}\ninput[type=range]::-webkit-slider-runnable-track {\n  width: 100%;\n  height: 12.8px;\n  cursor: pointer;\n  box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;\n  background: lightgray;\n  border-radius: 25px;\n  border: 0px solid #000101;\n}\ninput[type=range]::-webkit-slider-thumb {\n  box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;\n  border: 0px solid #000000;\n  height: 20px;\n  width: 20px;\n  border-radius: 50%;\n  background: var(--brand);\n  cursor: pointer;\n  -webkit-appearance: none;\n  margin-top: -3.6px;\n}\ninput[type=range]:focus::-webkit-slider-runnable-track {\n  background: lightgray;\n}\ninput[type=range]::-moz-range-track {\n  width: 100%;\n  height: 12.8px;\n  cursor: pointer;\n  animate: 0.2s;\n  box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;\n  background: lightgray;\n  border-radius: 25px;\n  border: 0px solid #000101;\n}\ninput[type=range]::-moz-range-thumb {\n  box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;\n  border: 0px solid #000000;\n  height: 20px;\n  width: 39px;\n  border-radius: 7px;\n  background: black;\n  cursor: pointer;\n}\ninput[type=range]::-ms-track {\n  width: 100%;\n  height: 12.8px;\n  cursor: pointer;\n  animate: 0.2s;\n  background: transparent;\n  border-color: transparent;\n  border-width: 39px 0;\n  color: transparent;\n}\ninput[type=range]::-ms-fill-lower {\n  background: lightgray;\n  border: 0px solid #000101;\n  border-radius: 50px;\n  box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;\n}\ninput[type=range]::-ms-fill-upper {\n  background: lightgray;\n  border: 0px solid #000101;\n  border-radius: 50px;\n  box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;\n}\ninput[type=range]::-ms-thumb {\n  box-shadow: 0px 0px 0px #000000, 0px 0px 0px #0d0d0d;\n  border: 0px solid #000000;\n  height: 20px;\n  width: 39px;\n  border-radius: 7px;\n  background: var(--brand);\n  cursor: pointer;\n}\ninput[type=range]:focus::-ms-fill-lower {\n  background: lightgray;\n}\ninput[type=range]:focus::-ms-fill-upper {\n  background: lightgray;\n}\n\n.priceTitle{\n  font-family: var(--serif);\n  font-size: 1.5em;\n  font-weight:200;\n  color:var(--brand)\n}\n\n.priceTitle span{\n  font-family: var(--sansSerif);\n  font-size: .7em;\n  font-weight:500;\n  color:black;\n}\n   \n.redBackground {\n  background-color: var(--brand);\n  height: 100vh;\n}\n\n.flex {\n  display: flex;\n  align-items: center;\n}\n\n.flexMain {\n  display: flex;\n  width: 90%;\n  margin: 0 auto;\n  padding-top: 100px;\n  align-content: space-between;\n}\n\n\n\n.collection_btn {\n  border: none;\n  /* background-color:#94154b; */\n  color: black;\n  /* padding:10px; */\n  height: 0em;\n  margin-left: -0.6em;\n  font-family: var(--sansSerif);\n  font-size: 0.56em;\n  padding: 0 10px 0 10px;\n  font-weight: 700;\n  cursor: pointer;\n  display: inline-block;\n}\n\n.collection_btn:hover {\n  color: var(--brand);\n}\n\na.purchase {\n  font-size: 0.7em;\n  color: var(--brand);\n  text-decoration: none;\n}\na.purchase:hover {\n  color: black;\n}\n\n/* .margin-bottom {\n  margin-bottom: 80px;\n} */\n\n.filter {\n  width: 25%;\n  margin-top: 10px;\n}\n\n.accMargin {\n  margin-bottom: 20px;\n}\n\n.wines-container {\n  width: 75%;\n  margin-left: 20px;\n\n  /* padding: 0px 30px 0px 30px; */\n}\n\n.wine-container {\n  background-color: white;\n  padding: 20px 40px 20px 0px;\n  margin-bottom: 30px;\n  box-shadow: 0px 0px 17px rgba(0, 0, 0, 0.05);\n}\n\nul {\n  list-style-type: none;\n  padding-inline-start: 0;\n  padding-top: 15px;\n  padding-bottom: 1px;\n}\n\n.img {\n  display: block;\n  max-width: 100%;\n  max-height: 17em;\n  margin: 0 auto;\n}\n\n.fade-enter-active,\n.fade-leave-active {\n  transition: opacity 0.3s;\n}\n.fade-enter, .fade-leave-to /* .fade-leave-active below version 2.1.8 */ {\n  opacity: 0;\n}\n\n.gray {\n  background-color: #f7f7f7;\n}\n\n.lower {\n  position: relative;\n  z-index: 0;\n}\n\n.higher {\n  width: 12em;\n  height: 2em;\n  position: fixed;\n  top: 50%;\n  left: 70%;\n  transform: translate(-50%, -50%);\n  z-index: 2;\n  font-family: var(--serif);\n  font-size: 2em;\n}\n\n.higherText {\n  background: var(--brand);\n  box-shadow: 0px 0px 10px rgb(172, 172, 172);\n  padding: 5px 15px 5px 15px;\n  border-radius: 10px;\n  color: white;\n}\n\n.rule {\n  height: 1.15em;\n  background-color: var(--brand);\n}\n\np {\n  font-family: var(--sansSerif);\n  font-weight: 400;\n  font-size: 1em;\n  line-height: 1.5em;\n}\n\n.rating {\n  border: 3.5px var(--brand) solid;\n  display: inline-block;\n  font-family: var(--sansSerif);\n  font-size: 1.15em;\n  font-weight: 700;\n  color: var(--brand);\n  padding: 5px 5px 5px 5px;\n  margin-top: 5px;\n}\n\n.inline {\n  display: inline-block;\n}\n\n.inside-rating {\n  font-family: var(--sansSerif);\n  background-color: var(--brand);\n  padding: 5px 5px 5px 5px;\n  margin-top: 5px;\n  font-size: 1.15em;\n  font-weight: 700;\n  color:white;\n  border: 3.5px var(--brand) solid;\n}\n\n.ratings {\n  font-family: var(--sansSerif);\n  color: var(--brand);\n  display: block;\n  margin-top: 6px;\n  margin-bottom: 17px;\n  font-size: 0.9em;\n}\n\n.price {\n  font-family: \"Abhaya Libre\", serif;\n  font-weight: 400;\n  font-size: 2em;\n  display: block;\n}\n\n.flex-container {\